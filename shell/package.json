{
  "version": "2.30.0",
  "name": "cn-shell",
  "description": "Cloud Native Shell",
  "main": "./dist/cn-shell.js",
  "bin": "",
  "files": [
    "dist/**/*"
  ],
  "prettier": {
    "trailingComma": "all"
  },
  "scripts": {
    "build-dev": "tsc --project tsconfig.dev.json",
    "build-prod": "npm run clean && tsc",
    "format-ts": "prettier --write 'src/**/*.ts'",
    "format-js": "prettier --write 'dist/**/*.{js,ts}'",
    "format-misc": "prettier --write '**/*.{json,md,yaml}'",
    "install-bin": "npm install -g $(npm pack . | tail -1)",
    "test": "echo \"No tests\" && exit 0",
    "clean": "rm -rf ./dist",
    "prepare": "npm run clean && npm run format-ts && npm run format-misc && tsc",
    "prepublishOnly": "npm run format-js && npm test"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "Cloud",
    "Native",
    "Application",
    "Framework",
    "CNA"
  ],
  "author": "Black Eye Technology",
  "license": "MIT",
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-helmet": "^6.0.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.3.0",
    "@types/luxon": "^3.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^12.20.55",
    "@types/shelljs": "^0.8.11"
  },
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "axios": "^0.25.0",
    "dotenv": "^16.0.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^4.0.1",
    "koa-helmet": "^4.2.1",
    "koa-router": "^7.4.0",
    "luxon": "^3.0.1",
    "minimist": "^1.2.6",
    "shelljs": "^0.8.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackeyetech/cn-shell-mono.git"
  },
  "bugs": {
    "url": "https://github.com/blackeyetech/cn-shell-mono/issues"
  },
  "homepage": "https://github.com/blackeyetech/cn-shell-mono/shell#readme"
}
